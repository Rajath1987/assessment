node {
    def app

    stage('Clone repository') {
             checkout scm
    }

    stage('Build image') {
        app = docker.build("<docker_ID>/assessment")
    }

   
    stage('Push image') {
            sh ' docker login -u <userName> -p <Password>'
            app.push("${env.BUILD_NUMBER}")
            app.push("latest")
       /* }*/
    }
}
